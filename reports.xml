<odoo>
    <report
        id="report_pasantias"
        model="pasantias.pasantias"
        string="Reporte de Pasantias"
        name="pasantias.report_pasantias"
        file="pasantias.report_pasantias"
        report_type="qweb-pdf" />

        <template id="pasantias.report_pasantias">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="o">
                    <t t-call="web.external_layout">
                        <div class="page">
                            <h2>Reporte de Pasantias</h2>
                            <p>PRUEBA<span t-field="o.name"/></p>
                        </div>
                    </t>
                </t>
            </t>
        </template>
</odoo>